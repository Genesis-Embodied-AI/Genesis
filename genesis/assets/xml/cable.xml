<mujoco model="cable">
    <default>
        <geom contype="0" conaffinity="0" group="0" />
        <!-- your contact type and what type of objects you collide with-->
        <default class="conty">
            <geom contype="1" conaffinity="0" condim="6" group="1" friction="10 10 10" solref="0.004 1" solimp="9.88 8 0.001"/>
        </default>
        <default class="conaff">
            <geom contype="0" conaffinity="1" condim="6" group="1" friction="0.4 0.002 0.00008" solref="0.004 1" />
        </default>
    </default>

    <worldbody>
        <body name="obj" pos="-0.38 -0.26 0.05">
            <joint name="obj" type="free" />
            <geom name="obj" priority="10" class="conty" rgba="0.2 0.2 0.9 1" type="sphere" size=".01" mass="0.0005"/>
            <site name='end2' rgba="0.2 0.2 0.9 1" type="sphere" size="0.002"/>
        </body>

        <body name="anchor" pos="-0.38 -0.26 1.07">
            <geom type="sphere" size="0.01" rgba="1 0 0 1" contype="0" conaffinity="0" />
        </body>

        <composite type="cable" count="26 1 1" curve="0 0 s" offset="-0.38 -0.26 0.06" initial="free">
            <joint kind="main" damping="0.01" armature="0.001"/>
            <geom type="capsule" size=".005 .007" rgba="0.8 0.6 0.4 1" mass="0.001"/>
        </composite>
    </worldbody>

    <equality>
        <weld body1="B_last" body2="anchor"/>
        <weld body1="B_first" body2="obj"/>
    </equality>
</mujoco>
